				"summary": "List licenses",
				"security": [
					{
						"api_key": []
					}
				],
				"parameters": [
					{
						"description": "Number of licenses to take.",
						"in": "query",
						"name": "take",
						"required": false,
						"schema": {
							"default": 10,
							"format": "int32",
							"type": "integer"
						}
					},
					{
						"description": "Number of licenses to skip.",
						"in": "query",
						"name": "skip",
						"required": false,
						"schema": {
							"default": 0,
							"format": "int32",
							"type": "integer"
						}
					},
					{
						"description": "Filter licenses by status.",
						"in": "query",
						"name": "filterStatus",
						"required": false,
						"schema": {
							"type": "string",
							"enum": [
								"active",
								"disabled/expired"
							]
						}
					},
					{
						"description": "Include logs for each license.",
						"in": "query",
						"name": "includeLogs",
						"required": false,
						"schema": {
							"default": false,
							"type": "boolean"
						}
					}
				]
			}
		},
