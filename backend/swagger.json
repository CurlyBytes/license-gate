							"application/json": {
								"schema": {
									"properties": {
										"count": {
											"type": "number",
											"format": "double"
										},
										"licenses": {
											"items": {
												"$ref": "#/components/schemas/License"
											},
											"type": "array"
										}
									},
									"required": [
										"count",
										"licenses"
